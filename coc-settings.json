{
    "suggest.noselect": false,
    "suggest.echodocSupport": true,
    "suggest.maxCompleteItemCount": 100,
    "diagnostic.infoSign": "✓",
    "diagnostic.hintSign": "◯",
    "coc.preferences.jumpCommand": "tab drop",
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "go",
        "dart",
        "cs",
        "java",
        "html",
        "javascript",
        "css",
        "vue",
        "svelte",
        "javascriptreact",
        "typescriptreact",
        "json"
    ],
    "go.goplsOptions": {
        "completeUnimported": true
    },
    "markdownlint.config": {
        "default": true,
        "line_length": false
    },
    "tabnine.priority": 30,
    "coc.source.word.priority": 20,
    "cSpell.userWords": [
        "dein"
    ]
}
